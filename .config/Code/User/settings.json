{
    "window.menuBarVisibility": "toggle",
    "window.title": "${rootName}${separator}${appName}",
    "workbench.sideBar.location": "left",
    "workbench.colorTheme": "Monokai Sharp",
    "workbench.tips.enabled": false,
    "workbench.activityBar.visible": false,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.startupEditor": "none",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.view.alwaysShowHeaderActions": true,
    "editor.fontSize": 16,
    "editor.cursorStyle": "line",
    "editor.fontFamily": "FuraMono Nerd Font, monospace",
    "editor.emptySelectionClipboard": true,
    "editor.rulers": [
        90
    ],
    "editor.wordWrapColumn": 89,
    "editor.suggestFontSize": 16,
    "editor.suggestSelection": "first",
    "editor.renderControlCharacters": false,
    "editor.largeFileOptimizations": false,
    "editor.tabSize": 2,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "git.autoRepositoryDetection": true,
    "git.path": "/usr/bin/git",
    "files.autoSave": "onFocusChange",
    "files.trimFinalNewlines": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.rightClickBehavior": "copyPaste",
    "debug.showInStatusBar": "never",
    "debug.console.fontFamily": "FuraMono Nerd Font, monospace",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "markdown.preview.breaks": true,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "liveshare.accountProvider": "github",
    "liveshare.autoShareServers": false,
    "liveshare.autoShareTerminals": false,
    "liveshare.guestApprovalRequired": true,
    "files.insertFinalNewline": true,
    "workbench.tree.renderIndentGuides": "none",
    "explorer.autoReveal": false,
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.visible": 5,
    "terminal.integrated.rendererType": "canvas",
    "editor.wordWrap": "wordWrapColumn",
    "git.confirmSync": false,
    "editor.glyphMargin": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "explorer.compactFolders": false,
    "git.autofetch": true,
    "terminal.external.linuxExec": "terminator",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "tabnine.experimentalAutoImports": true,
    "window.zoomLevel": 0,
    "githd.editor.disabled": true,
    "githd.logView.commitsCount": 100,
    "githd.logView.displayExpressStatus": false,
    "dashboard.projectData": null,
}
